@import url("https://fonts.googleapis.com/css?family=Lexend");
@import url("https://fonts.googleapis.com/css?family=Mouse+Memoirs");
body {
	font-family: 'Lexend' !important;
}

#opacheader,
#OpacNavRight,
#opaccredits,
#OpacMainUserBlock {
	display: none;
}

#login,
#moresearches,
#navigation,
#members,
#listsmenu,
#cart-list-nav,
#masthead_search,
#nologininstructions,
.rsssearchicon,
.breadcrumb,
#remove-selected,
label[for="masthead_search"] {
	display: none;
}

#opac-main #opac-main-search {
	margin-top: 10em;
    background: none;
}

#opac-main .container-fluid .row,
#results .container-fluid .row {
    align-items: center;
}

#libraryname {
    color: #56B4E9;
    font-size: 5vw;
}

#opac-main-search {
    background: none !important;
}

#searchform .form-row {
