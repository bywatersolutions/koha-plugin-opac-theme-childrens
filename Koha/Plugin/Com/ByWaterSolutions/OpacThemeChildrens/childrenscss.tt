}

#audience_heading,
.browse_heading {
	text-align: center;
    font-family: 'Mouse Memoirs' !important;
    color: #fff;
    font-size: 50px;
    border-bottom: 4px solid orange;
    text-shadow: 1px 1px 2px #000
}

.browse_heading {
    margin-bottom: 1em;
}

#opac-detail .main {
    border: 4px solid #000;
	background: #fff !important;
}

#views,
.isbn,
.publisher_number,
.tags,
.ddc,
.loc,
.oc,
.uniform_titles,
.tags,
.shelves,
.ratings,
.publisher,
.date,
.social_data,
