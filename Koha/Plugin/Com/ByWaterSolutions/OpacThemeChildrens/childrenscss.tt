      border-radius: 100px;
      margin-top: 1em;
      padding: 1.3em 0;
}

#searchsubmit {
    width: 100px;
    height: 100px;
    border-radius: 50%;
    background: #7a5cf0;
    display: flex;
    justify-content: center;
    align-items: center;
    color: #fff;
    margin-right: 15px;
    cursor: pointer;
}

#searchsubmit img {
    width: 100%;
}

#translControl1 {
    background: #fff;
    color: black;
    height: 100px;
    font-family: 'Lexend',sans-serif !important;
    font-size: xx-large;
    padding: 50px;
    outline: none;
    border: 0;
    border-radius: 50px;
    color: #000;
}

#translControl1::placeholder {
	color: #fff;
    font-weight: bold;
}

#search-facets,
#sort_by,
.addtoshelf,
.addtocart,
.selections-toolbar,
.selectcol,
.ratings,
.suggestion,
#toolbar {
	display: none !important;
}

body,
.main,
#results tbody,
#opac-userlists tbody,
#opac-lists tbody {
    background: #0072b2;
}

#results .author li {
    display: inline;
}

.main {
	background: none !important;
}

#results table tbody,
#opac-userlists table tbody,
#opac-lists table tbody {
	display: grid;
    grid-template-columns: 50% 50%;
    grid-gap: 10px;
}

#results table tbody tr,
#opac-userlists table tbody tr,
