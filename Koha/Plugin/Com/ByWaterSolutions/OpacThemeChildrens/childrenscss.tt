    box-shadow: 0 0 5px -1px black, inset -1px 1px 2px rgba(255, 255, 255, 0.5);
    margin: auto;
    border-radius: 5px;
}

.bibliocol {
    display: block;
	background: #fff !important;
}

.title_summary {
    font-size: 140%;
    padding-top: 10px;
}

.actions a, .reserve {
    background: #ff951c;
    color: #fff !important;
    margin-bottom: 10px;
    font-size: 22px;
    font-weight: bolder;
}

.series {
    padding-top: 1em;
}

.series a {
    background: green;
    color: #fff !important;
    margin-bottom: 10px;
    font-size: 18px;
    font-weight: bolder;
    padding: 5px;
    border-radius: 4px;
}

.online_resources {
	padding-bottom: 10px;
	padding-top: 10px;
}

.online_resources a {
    background: purple;
    color: #fff !important;
    margin-bottom: 10px;
    font-size: 18px;
    font-weight: bolder;
    padding: 5px;
    border-radius: 4px;
}

.label {
	font-weight: bolder;
}

.pagination {
	display: block;
}

.pg_menu {
    background: none;
    border-top: none;
    margin-top: 25px;
}

.pg_menu .pg_link.back_results a {
    border-left: 4px solid #fff;
    border-right: 4px solid #fff;
}

.page-link[aria-label^="Go to page "],
.page-link[aria-label^="Current"],
.page-link[aria-label^="Go to the last"],
.page-link[aria-label^="Go to the first"] {
	display: none;
}

.page-link[aria-label="Go to the next page"],
.right_results a {
    background: #c1cd23 !important;
    color: #fff;
    font-size: 30px !important;
    border: 4px solid #fff;
    box-shadow: 2px 3px 4px rgba(0, 0, 0, 0.25);
    padding: .25rem .5rem !important;
    float: right;
}

.page-link[aria-label="Go to the previous page"],
.left_results a,
.left_results span {
    background: #b32317 !important;
    color: #fff;
    font-size: 30px !important;
    border: 4px solid #fff;
    box-shadow: 2px 3px 4px rgba(0, 0, 0, 0.25);
    padding: .25rem .5rem !important;
    float: left;
}

.back_results a {
    background: #3740c9 !important;
    color: #fff;
    font-size: 30px !important;
    border: 4px solid #fff;
    box-shadow: 2px 3px 4px rgba(0,0,0,0.25);
    padding: .25rem .5rem !important;
}

.page-link .fa,
.pg_menu .fa {
	color: #fff !important;
	font-size: 30px;
    text-shadow: 1px 1px 2px #000;
}

#toolbar {
	background: none;
    border: none;
}

#floating {
	box-shadow: none;
    width: 100%;
}

.availability > .label {
	display: block;
    font-size: 160%;
    padding-top: 10px;
    padding-bottom: 10px;
}

.available .AvailabilityLabel {
	display: block;
    font-weight: bold;
}

.ItemSummary {
	display: block;
}

.materialtype {
	float: right;
    margin: -14px;
    margin-right: -13px;
    padding: 10px;
    border-top: 1px solid #000;
	border-right: 1px solid #000;
    text-shadow: 1px 1px 2px #000;
    color: #0072B2;
    opacity: .15;
    font-size: 200px;
}

.no-image {
    display: none;
}

.results_material_type .label {
	display: none;
}

.type {
	display: none;
}

#kids_head {
    font-size: 50px;
    font-family: 'Mouse Memoirs' !important;
    line-height:49px;
	text-align: center;
    padding-top: 30px;
	padding-bottom: 15px;
    text-shadow: 1px 1px 2px #000;
}

.welcome {
    color: #ff951c;
}

.kids_library > span {
    color: #c1cd23;
    font-size: 80px;
}

#kids_browse {
	display: grid;
	grid-template-columns: repeat(5, 150px);
    grid-gap: 2vw;
